{
    "index_name": "LoM_wiki",
    "start_urls": ["https://smilin.net/LoM-wiki/"],
    "sitemap_urls": ["https://smilin.net/LoM-wiki/sitemap.xml"],
    "sitemap_alternate_links": true,
    "stop_urls": [],
    "selectors": {
      "lvl0": {
        "selector": ".sidebar-heading.active, .VPNavBarMenuLink.active, .VPNavBarMenuGroup .VPNavBarMenuGroupButton.active",
        "global": true,
        "default_value": "Documentation"
      },
      "lvl1": ".content h1",
      "lvl2": ".content h2",
      "lvl3": ".content h3",
      "lvl4": ".content h4",
      "lvl5": ".content h5",
      "content": ".content div, .content p, .content li, .content span, .content td, .content th, .content code, .content pre",
      "lang": {
        "selector": "/html/@lang",
        "type": "xpath",
        "global": true
      },
      "image_url": {
        "selector": ".content img",
        "type": "css",
        "attribute": "src"
      },
      "image_alt": {
        "selector": ".content img",
        "type": "css",
        "attribute": "alt"
      }
    },
    "custom_settings": {
      "attributesForFaceting": ["lang"],
      "attributesToRetrieve": [
        "hierarchy",
        "content",
        "anchor",
        "url",
        "image_url",
        "image_alt"
      ],
      "attributesToSnippet": ["content:10"]
    },
    "strip_chars": " .,;:#",
    "scrape_start_urls": false
  }